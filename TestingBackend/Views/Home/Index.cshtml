<head>
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
	<link rel="stylesheet" href="~/Content/styles.css">
</head>
<body>
	<script defer src="https://use.fontawesome.com/releases/v5.15.4/js/all.js" integrity="sha384-rOA1PnstxnOBLzCLMcre8ybwbTmemjzdNlILg8O7z1lUkLXozs4DHonlDtnE7fpc" crossorigin="anonymous"></script>
	<div class="row">
		<div class="col-lg-8">
			<div class="titulo">
				<h2>
					Listado de Formularios de solicitud de registro
				</h2>
			</div>
		</div>
	</div>
	<hr class="linea">
	<button type="button" class="btn btn-secondary" data-toggle="tooltip" data-placement="right"
			title="Crear" onclick="@("window.location.href='" + @Url.Action("Create", "Home") + "'");" >
		Crear
	</button>
	<div class="tabla">
		<table class="table table-bordered">
			<thead class="borders">
				<tr class="titulos">
					<th scope="col">ID</th>
					<th scope="col">Código</th>
					<th scope="col">Nombre</th>
					<th scope="col">Dirección</th>
					<th scope="col">Acciones</th>
				</tr>
			</thead>
			<tbody class="items">
				<tr>
					<td class="id">ID DE EJEMPLO</td>
					<td class="codigo">COD DE EJEMPLO</td>
					<td class="nombre">
						NOMBRE DE EJEMPLO
					</td>
					<td class="direccion">DIRECCION DE EJEMPLO</td>
					<td class="acciones">
						<span class="span">
							<button type="button" class="btn btn-secondary" data-toggle="tooltip"
									data-placement="right" title="Editar">
								<i class="fas fa-pencil-alt edit"></i>
							</button>
						</span>
						<span class="span">
							<button type="button" class="btn btn-secondary" data-toggle="tooltip"
									data-placement="right" title="Ver">
								<i class="fas fa-search edit"></i>
							</button>
						</span>
						<span class="span">
							<button type="button" class="btn btn-secondary" data-toggle="tooltip"
									data-placement="right" title="Eliminar">
								<i class="fas fa-trash-alt edit"></i>
							</button>
						</span>
					</td>
				</tr>
			</tbody>
		</table>
	</div>

</body>
